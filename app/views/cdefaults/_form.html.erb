<%= bootstrap_form_for(@cdefault, url: path) do |f| %>
  <%= f.text_field :name %> 
  <%= f.select :type, [["Income Category","Icdefault"], ["Expense Category","Ecdefault"]] %> 
  <%= f.check_box :default %>
  <%= f.submit "Send"%> 
<% end %>
<%= link_to "Volver", admin_index_path, :class => "btn btn-primary" %>